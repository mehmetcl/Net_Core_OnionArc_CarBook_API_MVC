@model ResultCreateFeatureDto
@{
    ViewData["Title"] = "CreateFeature";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="container-fluid">

    <!-- start page title -->
    <div class="row">
        <div class="col-xl-12">

            <div class="card">
                <div class="card-body">

                    <h4 class="card-title">Yeni Araç ÖzelliğiGirişi</h4>
                    <p class="card-subtitle mb-4">Araç ile ilgili Özellikleri giriniz.. </p>

                    <form method="post">
                        
                        <div class="form-group">
                            <label for="exampleInputFeature">Özellik Adı</label>
                            <input type="text" class="form-control" id="exampleInputFeature" asp-for="FeatureName" placeholder="Özellik girişi Yapınız">
                        </div>
                      
                        <button type="submit" class="btn btn-primary waves-effect waves-light">Kaydet</button>
                    </form>

                </div> <!-- end card-body-->
            </div>

        </div>
        <!-- end card-body-->
    </div>
    <!-- end card -->

</div>

